{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard",
    ":disableRateLimiting",
    ":semanticCommits",
    ":semanticCommitScope(deps)"
  ],
  "automerge": true,
  "automergeStrategy": "rebase",
  "rangeStrategy": "bump"
}
